{
  "name": "reason-loadable",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["reason-react"],
  "bs-dev-dependencies": ["@glennsl/bs-jest", "bs-react-testing-library"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3
}
